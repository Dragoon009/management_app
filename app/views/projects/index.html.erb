<table>
  <tr>
    <th>Projects</th>


    <th colspan="2"></th>
  </tr>

  <% @projects.each do |project| %>
    <tr>
      <td><%= project.name %></td>
      <td><%= project.skills.map {|s| s.skill}.join(', ') %></td>
      <td><%= link_to "Show", project_path(project)  %></td>
      <td><%= link_to "Edit",edit_project_path(project) %></td>
      <td><%= link_to 'Delete', project ,method: :delete,data: { confirm: 'Are you sure?' } %></td>
    </tr>
  <% end %>
</table>
<%= link_to "Create new project", {controller: 'projects', action: 'new'} %>
<br>
<%= link_to "Back", current_user %>
