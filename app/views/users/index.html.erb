<table>
  <tr>
    <th>Users</th>
    <th colspan="2"></th>
  </tr>
	<% @users.each do |user| %>
  	<tr>
    	<td><%= user.name %></td>
      <td><%= user.email%></td>
      <td><%= user.skills.map {|s| s.skill}.join(', ') %></td>
      </td>
    	<td><%= link_to "delete", user, method: :delete,data: { confirm: "You sure?" } if !user.admin?%> </td>
    </tr>
  <% end %>
</table>
